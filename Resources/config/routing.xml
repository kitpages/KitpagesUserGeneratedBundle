<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="KitpagesUserGeneratedBundle_comment_doNewPost" pattern="/do-new-post">
        <default key="_controller">KitpagesUserGeneratedBundle:Comment:doNewPost</default>
    </route>
    <route id="KitpagesUserGeneratedBundle_comment_ajaxNextStatus" pattern="/ajax-next-status">
        <default key="_controller">KitpagesUserGeneratedBundle:Comment:ajaxNextStatus</default>
    </route>

    <route id="KitpagesUserGeneratedBundle_rating_doNewScore" pattern="/do-new-score">
        <default key="_controller">KitpagesUserGeneratedBundle:Rating:doNewScore</default>
    </route>

    <route id="KitpagesUserGeneratedBundle_rating_initRatingCache" pattern="/initRatingCache">
        <default key="_controller">KitpagesUserGeneratedBundle:Rating:initRatingCache</default>
    </route>

</routes>
